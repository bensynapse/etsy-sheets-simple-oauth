# Etsy API Configuration
# Copy this file to .env and fill in your values

# Etsy API Key (also called Keystring or Client ID)
# Get from: https://www.etsy.com/developers/your-apps
ETSY_API_KEY=your_api_key_here

# Encryption key for secure credential storage
# This will be auto-generated if not provided
# Keep this secret and never commit to version control!
ENCRYPTION_KEY=

# OAuth Redirect URI (keep as localhost for manual auth)
REDIRECT_URI=http://localhost:8888/oauth/redirect

# Optional: Debug mode
DEBUG=false

# Optional: Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO